  OAUTH2
  BASIC_AUTH
  CERTIFICATE
  OIDC
  PKI
  OIDC_PKI
  NONE
}

enum ExternalServiceStatus {
  active
  inactive
  error
  syncing
}

enum AdaptorSyncType {
  METADATA
  SEISMIC_DATA
  WELL_LOGS
  RESERVOIR_DATA
  FULL_SYNC
  INCREMENTAL
}

enum SyncStatus {
  in_progress
  completed
  failed
  cancelled
}

enum DataspaceConnectorStatus {
  online
  offline
  synchronizing
  error
}

enum RoutingType {
  ROUND_ROBIN
  WEIGHTED
  FAILOVER
  RANDOM
}

enum LoadBalancingType {
  ROUND_ROBIN
  LEAST_CONNECTIONS
  IP_HASH
