  running
  completed
  failed
  cancelled
}

enum ExternalServiceType {
  IDS_BROKER
  DATA_CATALOG
  AUTHENTICATION
  MONITORING
  ANALYTICS
  OGC_OSDU_ADAPTOR
  DATA_PROVIDER
  METADATA_BROKER
}

enum AuthenticationType {
